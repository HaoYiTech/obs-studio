
<block wx:if="{{m_show_auth}}">
  <wux-result
      icon="{{ icon }}"
      title="{{ m_title }}"
      label="{{ m_label }}"
      buttons="{{ buttons }}"
      extra="Copyright © 2019 启迪未来"
      fixed="true"
      bind:getuserinfo="getUserInfo"
  />
</block>
<block wx:else>
  <view style="margin-top:120px;">
    <i-load-more loading="{{!m_show_auth}}" tip="正在加载..." />
  </view>
</block>